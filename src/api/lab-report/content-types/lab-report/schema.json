{
  "kind": "collectionType",
  "collectionName": "lab_reports",
  "info": {
    "singularName": "lab-report",
    "pluralName": "lab-reports",
    "displayName": "Lab Report"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "subject": {
      "type": "string",
      "required": true,
      "maxLength": 12,
      "minLength": 4,
      "regex": "^\\w+\\s\\-\\s\\d{3}$"
    },
    "about": {
      "type": "text",
      "required": true
    },
    "due": {
      "type": "datetime",
      "required": true
    }
  }
}
